<!DOCTYPE html>
<html lang="en">
  <body>
      <% include partials/_header %>
      <ul>
        <% for(var i in urls) { %>
          <li><%= i + " URL:  " + urls[i] %>  </li>
          <a href="/urls/<%= i %>">Edit</a>
          <form method="POST" action="/urls/<%= i %>/delete">
            <button type='submit'>Delete</button>
          </form>
          <link rel="stylesheet" type="text/css" href="">
        <% } %>
      </ul>

  </body>
</html>
